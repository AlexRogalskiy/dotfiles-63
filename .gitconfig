[core]
	excludesfile = /Users/b/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
	ignorecase = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[interactive]
	diffFilter = diff-so-fancy --patch
